{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "Node16",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "removeComments": true,
    "resolveJsonModule": false,
    "sourceMap": true,
    "strict": true,
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "incremental": true
  },
  "exclude": [".eslintrc"],
  "ts-node": {
    "swc": true
  }
}
